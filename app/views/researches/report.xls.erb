<% case @report_type%>
	<% when "global" %>
		<%=  render partial: "/researches/reports/global_report" %>
	<% when "global_dimensions" %>
		<%=  render partial: "/researches/reports/global_dimensions_report" %>
	<% when "dimension_demographic" %>
		<%=  render partial: "/researches/reports/dimension_demographic_report" %>
	<% when "dimension_questions" %>
		<%=  render partial: "/researches/reports/dimension_questions_report" %>
	<% when "dimension_questions_demographic" %>
		<%=  render partial: "/researches/reports/dimension_questions_demographic_report" %>
	<% when "global_questions" %>
		<%=  render partial: "/researches/reports/global_questions_report" %>
	<% when "questions_demographic" %>
		<%=  render partial: "/researches/reports/questions_demographic_report" %>
<% end %>